{
	"name": "pacfw",
	"version": "0.0.1",
	"description": "A simple PAC For WSH",
	"main": "dist/index.js",
	"private": true,
	"scripts": {
		"build": "npx tsc",
		"build:watch": "npx tsc -w",
		"debug:jscript": "cscript //X dist/index.js",
		"debug:v8": "node inspect dist/index.js",
		"test": "npx jest",
		"test:watch": "npx jest --watch",
		"coverage": "npx jest --all --coverage",
		"lint": "npx eslint {test,src}/**/*.ts",
		"coveralls": "cat ./coverage/lcov.info | coveralls",
		"prepack": "npm run build"
	},
	"author": "Komsomolsk Inari (https://github.com/komsomolskinari)",
	"repository": {
		"type": "git",
		"url": "https://github.com/komsomolskinari/fwpac"
	},
	"files": [
		"/dist"
	],
	"license": "0BSD",
	"devDependencies": {
		"@types/jest": "^24.0.23",
		"@types/mime-types": "^2.1.0",
		"@types/node": "^12.12.18",
		"@typescript-eslint/eslint-plugin": "^2.12.0",
		"@typescript-eslint/parser": "^2.12.0",
		"coveralls": "^3.0.9",
		"eslint": "^6.7.2",
		"eslint-config-prettier": "^6.7.0",
		"eslint-plugin-prettier": "^3.1.2",
		"jest": "^24.9.0",
		"prettier": "^1.19.1",
		"ts-jest": "^24.2.0",
		"typescript": "^3.7.3"
	},
	"dependencies": {
		"mime-types": "^2.1.25"
	}
}
